PROCESS BEFORE OUTPUT.
*&SPWIZARD: PBO FLOW LOGIC FOR TABLECONTROL 'TC_WBS'
  MODULE TC_WBS_CHANGE_TC_ATTR.
*&SPWIZARD: MODULE TC_WBS_CHANGE_COL_ATTR.
  LOOP AT   GS_WBS_DISPLAY
       INTO GS_WBS_DISPLAY
       WITH CONTROL TC_WBS
       CURSOR TC_WBS-CURRENT_LINE.
    MODULE TC_WBS_GET_LINES.
*&SPWIZARD:   MODULE TC_WBS_CHANGE_FIELD_ATTR
  ENDLOOP.

  MODULE status_0100.
  LOOP AT gt_wbs_display INTO gs_wbs_display
    WITH CONTROL tc_wbs CURSOR tc_wbs-current_line.
    MODULE fill_table_control.
  ENDLOOP.

PROCESS AFTER INPUT.
*&SPWIZARD: PAI FLOW LOGIC FOR TABLECONTROL 'TC_WBS'
  LOOP AT GS_WBS_DISPLAY.
    CHAIN.
      FIELD GS_WBS_DISPLAY-STUFE.
      FIELD GS_WBS_DISPLAY-POSID.
      FIELD GS_WBS_DISPLAY-POST1.
      FIELD GS_WBS_DISPLAY-KOSTL.
      FIELD GS_WBS_DISPLAY-ZAPPROVED_BUDG.
      FIELD GS_WBS_DISPLAY-ZBUDGET_CHECK.
      FIELD GS_WBS_DISPLAY-IMPRF.
      FIELD GS_WBS_DISPLAY-PRART.
      FIELD GS_WBS_DISPLAY-MARK.
      MODULE TC_WBS_MODIFY ON CHAIN-REQUEST.
    endchain.
    FIELD GS_WBS_DISPLAY-POSNR
      MODULE TC_WBS_MARK ON REQUEST.
  ENDLOOP.
  MODULE TC_WBS_USER_COMMAND.
*&SPWIZARD: MODULE TC_WBS_CHANGE_TC_ATTR.
*&SPWIZARD: MODULE TC_WBS_CHANGE_COL_ATTR.

  MODULE user_command_0100.
  LOOP AT gt_wbs_display.
    MODULE modify_table_control.
  ENDLOOP.
